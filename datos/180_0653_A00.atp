/* STANDARD TEST FAIL SUBROUTINE */

 SUBROUTINE FAILSUB();
    DECLARE CSTRING UMSG(8);
    DECLARE CSTRING UNIT(1);

    LET UNIT=SUBSTR(FTYPE,1,1);
    IF UNIT='R' THEN LET UMSG='OHMS';
    IF UNIT='C' THEN LET UMSG='FARADS';
    IF UNIT='L' THEN LET UMSG='HENRIES';
    IF UNIT='V' THEN LET UMSG='VOLTS';
    IF UNIT='Z' THEN LET UMSG='OHMS';
    IF UNIT='X' THEN LET UMSG='OHMS';
    IF UNIT='Y' THEN LET UMSG='MHOS';
    IF FCMSG <> '' THEN
	WRITE ID=MESFILE '%037%%S%%035%(%S%)%NL%' FCMSG,FLABEL;
    ELSE
	WRITE ID=MESFILE '%037%%S%%NL%' FLABEL;
    WRITE ID=MESFILE '%036%  EXPECTED        ACTUAL%NL%';
    WRITE ID=MESFILE '  %S% %S%       ' FDMSG,UMSG;
    WRITE ID=MESFILE '%5.2G% %S%%NL%' FMEAS,UMSG;
    WRITE ID=MESFILE '   LIMITS ARE ';
    WRITE ID=MESFILE '%5.2G% AND %5.2G% %S%' FLO,FHI,UMSG;
    WRITE ID=MESFILE '%035%%NL%';
    IF FPMSG <> '' THEN [
	WRITE ID=MESFILE '%S% %NL%' FPMSG;
	WRITE ID=LOGFILE '=%S%' FPMSG;];
    IF SHOW_TEST_NODES_USED = 'ON' THEN [
      WRITE ID=MESFILE 'CHAN A/E: %S%%NL%' FSRCNODEMSG;
      WRITE ID=MESFILE 'CHAN B/F: %S%%NL%' FMEASNODEMSG;
      WRITE ID=MESFILE 'CHAN C/G/GND: %S%%NL%' FGRDNODEMSG;
    ];
    WRITE ID=MESFILE '%NL%';
  END SUBROUTINE FAILSUB;





 

