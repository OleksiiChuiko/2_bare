SUBROUTINE AUTOCAL();

 DECLARE SYSTEM CNTRL_C(CSTRING CONTROL(8));
 DECLARE BSTRING FIXT1(16);
 DECLARE BSTRING FIXT2(16);
 DECLARE BSTRING ORIG(8);
 DECLARE FLOAT A;

  /** Make sure operator cannot ABORT this Subroutine **/
  CALL CNTRL_C(CONTROL='OFF'); /* disable control C processing */

 /* Disable Logging during Autocal Subroutine */
 NOLOG;

/*   IF EITHER ORIG(1) OR ORIG(2) IS SET, */
/*   VAC MUST BE IN 'AUTO' OR 'ON' MODE. */

/*   IF NEITHER IS SET, THIS MEANS VAC MUST BE 'OFF'.    */
/*   NO PROMPTING IS REQUIRED BEFORE MEASURING RESIDUALS */
/*   IT ALSO MEANS FIXTURE DOWN WIRING IS UNKNOWN */
    
  MEAS PIO(0) HRLY INTO ORIG;
  IF ORIG(1) THEN BRANCH CHKSW; 
  IF ORIG(2) THEN BRANCH CHKSW;
  LET A = -1.0 ;
  BRANCH SETCAL; /* IF VAC WAS OFF, SKIP THE PRE-TEST PROMPTING */

CHKSW:
  MEAS PIO(0) SWITCH INTO FIXT1;
  IF FIXT1(1) THEN BRANCH SWLP1;
  IF FIXT1(2) THEN BRANCH SWLP1;
  LET A = 0.0 ; 
  BRANCH PRE_PROMPT; /* FIX DN SWITCHES ARE OPEN, PROMPT USER */

SWLP1:
  SET PIO(0) HRLY(OPEN 1,2);
  FOR A=0 TO 10 DO [
    DELAY .05;
    MEAS PIO(0) SWITCH INTO FIXT2;
    IF FIXT2 != FIXT1 THEN [
      LET A = 1.0 ;
      BRANCH SETCAL;
    ];
  ];

LET A = 0.0 ; /* NOTHING HAPPENED */


PRE_PROMPT:
/* AUTOMATIC CALIBRATION OF RESIDUAL VALUES */

 
